{
  "name": "box-file-picker",
  "version": "1.0.0",
  "description": "Custom form field for Kissflow's form",
  "main": "index.js",
  "scripts": {
    "dev": "form-field-scripts dev",
    "build": "form-field-scripts build",
    "zip": "rm -rf ./box-file-picker.zip ./dist && npm run build && cd ./dist && zip -q -r ../box-file-picker.zip . && rm -rf ../dist",
    "unzip": "rm -rf ./box-file-picker && unzip box-file-picker.zip -d ./box-file-picker"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@kissflow/form-field-scripts": "workspace:^",
    "@kissflow/form-field-config": "workspace:^",
    "unzipper": "^0.10.14",
    "zip": "^1.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {}
  },
  "keywords": [
    "lcnc",
    "no-code",
    "low-code",
    "platform",
    "react",
    "cli",
    "kissflow",
    "custom-components",
    "custom-fields"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
